public class amstrong {
    public static void main(String[] args) {
    	
        Scanner sc = new Scanner(System.in);

        System.out.print("Enter a num: ");
        int num = sc.nextInt();

        int temp = num;
        int sum = 0;

        while (temp > 0)
        {
            int dig = temp % 10;
            sum += dig * dig * dig;
            temp = temp / 10;
        }

        if (sum == num)
        {
            System.out.println(num + " is an amstrong num");
        } else 
        {
            System.out.println(num + " is not an amstrong num");
        }

        sc.close();
    }



	}


