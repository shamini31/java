package day4;
import java.util.Scanner;
public class count {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int start= sc.nextInt();
		int end= sc.nextInt();
		int evencount=0;
		for(int i=start;i<=end;i++);
		{
			if(i%2!=0)
			{
				evencount++;
			}
		}			
		System.out.println(evencount);
		
			
		}
	

	}

}
