package day6;
import java.lang.String;
public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StringBuffer sb= new StringBuffer("Hello");
		System.out.println(sb);
		sb.append(", World ! ");
		System.out.println(sb);
		sb.insert(5, "java ");
		System.out.println(sb);
		sb.delete(5, 10);
		System.out.println(sb);
		sb.reverse();
		System.out.println(sb);
		sb.replace(0,3," AA ");
		System.out.println(sb);

	}

}
